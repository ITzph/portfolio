    "nx": "nx",
    "start": "ng serve",
    "dev": "ng serve --configuration hmr -o --proxy-config apps/webapp/proxy.conf.json",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
